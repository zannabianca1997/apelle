{
    "name": "apelle-webui",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "svelte-kit sync && orval && vite dev",
        "build": "svelte-kit sync && orval && vite build",
        "preview": "svelte-kit sync && orval && vite preview",
        "check": "svelte-kit sync && orval && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && orval && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "orval": "orval"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@iconify-json/mdi": "^1.2.3",
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^15.14.0",
        "orval": "^7.7.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "sass": "^1.86.0",
        "svelte": "^5.25.3",
        "svelte-check": "^4.0.0",
        "sveltekit-sse": "^0.13.16",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "unplugin-icons": "^22.1.0",
        "vite": "^5.4.11"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "dayjs": "^1.11.13",
        "svelte-i18n": "^4.0.1"
    }
}
